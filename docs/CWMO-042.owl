    <!-- http://purl.org/cwmo/#adviceAdressedTo -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#adviceAdressedTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#Advice"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#CreativeTeam"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#advicedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#advicedBy">
        <owl:inverseOf rdf:resource="http://purl.org/cwmo/#provideAdvice"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#Advice"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#Expert"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#appliedInContextType -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#appliedInContextType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#CreativeTechnique"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#ContextType"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#applyEvaluationThanks -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#applyEvaluationThanks">
        <owl:inverseOf rdf:resource="http://purl.org/cwmo/#isEvaluatedDuring"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#EvaluationTechnic"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#EvaluationCriteria"/>
        <rdfs:comment xml:lang="en">Evaluation technic apply evaluation criteria to do the evaluation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#coverDomain -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#coverDomain">
        <owl:inverseOf rdf:resource="http://purl.org/cwmo/#isCoveredBy"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/cwmo/#Problem"/>
                    <rdf:Description rdf:about="http://purl.org/cwmo/#Subject"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#KnowledgeDomain"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#criteriaIsDefinedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#criteriaIsDefinedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#EvaluationCriteria"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/cwmo/#DecisionMaker"/>
                    <rdf:Description rdf:about="http://purl.org/cwmo/#Organizer"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Evaluation criteria is defined by the decision maker and/or the organizer.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#equipe -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#equipe">
        <owl:inverseOf rdf:resource="http://purl.org/cwmo/#requireEquipment"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#Equipment"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#CreativeTechnique"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#evalStratPlannedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#evalStratPlannedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#EvaluationStrategy"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/cwmo/#DecisionMaker"/>
                    <rdf:Description rdf:about="http://purl.org/cwmo/#Organizer"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#evalTechAppliedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#evalTechAppliedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#EvaluationTechnic"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#Evaluator"/>
        <rdfs:comment xml:lang="en">Evaluation technic is applied by evaluator.</rdfs:comment>
        <rdfs:comment>restreindre la relation en fonction de la strategy: TechA evalTechappliedBy Anon for a given strategy</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#evaluateResponseTo -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#evaluateResponseTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#EvaluationStrategy"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#Subject"/>
        <rdfs:comment xml:lang="en">An evaluation strategy evaluate the ideas associated to a subject. The strategy is dependant of the differnt element that define the subject (context, decision-maker, ...)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasApplicationMode -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasApplicationMode">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#CreativeTechnique"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#ApplicationMode"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasAuthor -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasAuthor">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#Idea"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#Ideator"/>
        <rdfs:comment xml:lang="en">An idea is written by an ideator.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasCTSource -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasCTSource">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#Idea"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#CreativeTechnique"/>
        <rdfs:comment xml:lang="en">An idea has been produced during the application of a creative technic.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasCWSource -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasCWSource">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#Idea"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#CreativeWorkshop"/>
        <rdfs:comment xml:lang="en">An idea has been produced during the a creative workshop.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasContext -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasContext">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#Problem"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#Context"/>
        <rdfs:comment xml:lang="en">A problem is defined according to a context.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasContextType -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasContextType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#Context"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#ContextType"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasCreatTechnic -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasCreatTechnic">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#CreativeStrategy"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#CreativeTechnique"/>
        <rdfs:comment xml:lang="en">Creative Strategy is composed by one or several creative techniques</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasCreativePhase -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasCreativePhase">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#CreativeTechnique"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#CreativePhase"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasEquivalent -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasEquivalent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#KnowledgeDomain"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#KnowledgeDomain"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasEvalStrat -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasEvalStrat">
        <owl:inverseOf rdf:resource="http://purl.org/cwmo/#hasEvalTechnic"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#EvaluationTechnic"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#EvaluationStrategy"/>
        <rdfs:comment xml:lang="en">Evaluation technic is part of an evaluation strategy.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasEvalTechnic -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasEvalTechnic">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#EvaluationStrategy"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#EvaluationTechnic"/>
        <rdfs:comment xml:lang="en">Evaluation strategy is composed by evaluation technic.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasExpressionMode -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasExpressionMode">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#CreativeTechnique"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#ExpressionMode"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasFunction -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasFunction">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#CreativeTechnique"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#TechniqueFunction"/>
        <rdfs:comment xml:lang="en">A creative method produce different results which can be classified according to these typology: exploration, generation, and/or evaluation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasGeneralization -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasGeneralization">
        <owl:inverseOf rdf:resource="http://purl.org/cwmo/#hasSpecialization"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#KnowledgeDomain"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#KnowledgeDomain"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasGenerated -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasGenerated">
        <owl:inverseOf rdf:resource="http://purl.org/cwmo/#hasGroupAuthor"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#CreativeTeam"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#Idea"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasGroupAuthor -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasGroupAuthor">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#Idea"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#CreativeTeam"/>
        <rdfs:comment xml:lang="en">Idea is generated into and by a team.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasHypernym -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasHypernym">
        <owl:inverseOf rdf:resource="http://purl.org/cwmo/#hasHyponym"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:comment xml:lang="en">Concept whose semantic field is broader than that of a hyponym.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasHyponym -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasHyponym">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:comment xml:lang="en">Concept whose semantic field is more specific than its hypernym</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasIdeator -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasIdeator">
        <owl:equivalentProperty rdf:resource="http://xmlns.com/foaf/0.1/#member"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#CreativeTeam"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#Ideator"/>
        <rdfs:comment xml:lang="en">A creative Team is composed by several ideator</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasInputType -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasInputType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#CreativeTechnique"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#InputType"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasKnowledge -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasKnowledge">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#KnowledgeDomain"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasNext -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasNext">
        <owl:inverseOf rdf:resource="http://purl.org/cwmo/#hasPrevious"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/cwmo/#Activity"/>
        <rdfs:range rdf:resource="http://purl.org/cwmo/#Activity"/>
        <rdfs:comment xml:lang="en">The creative technic can be follewed by another creative technic.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cwmo/#hasParticipant -->

    <owl:ObjectProperty rdf:about="http://purl.org/cwmo/#hasParticipant">
        <owl:inverseOf rdf:resource="http://purl.org/cwmo/#isParticipatingTo"/>
